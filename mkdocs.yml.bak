# mkdocs.yml 核心配置
site_name: 我的项目文档  # 你的文档网站名称（必填）
theme:
  name: material  # 【指定启用 Material 主题】
  # （可选）添加主题个性化配置，如颜色、字体等
  palette:
    primary: "blue"  # 主色调（可选值：red/indigo/purple/green 等）
    accent: "light blue"  # 强调色
  features:
    - navigation.tabs  # 启用顶部标签导航
    - navigation.sections  # 启用侧边栏章节折叠
    - search.suggest  # 搜索建议
    - search.highlight  # 搜索结果高亮
